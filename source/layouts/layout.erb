    <%= yield %>
    <%= javascript_include_tag "bundle" %>
    <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
    <script>
      AOS.init({
        // Settings that can be overridden on per-element basis, by `data-aos-*` attributes:
